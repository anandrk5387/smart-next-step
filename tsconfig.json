    "rootDir": "lambdas",
    "outDir": "dist/lambdas",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["lambdas/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
